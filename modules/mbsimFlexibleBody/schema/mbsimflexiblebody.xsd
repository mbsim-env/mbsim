  <xs:element name="FlexibleFfrBody" substitutionGroup="mbsim:Body" type="FlexibleFfrBodyType">
    <xs:annotation>
      <xs:documentation xml:lang="de" xmlns="">
        Definiert einen elastischen Körper mit einer Frame Of Reference Formulierung.<br/>
      </xs:documentation>
    </xs:annotation>
  </xs:element>
  <xs:complexType name="FlexibleFfrBodyType">
    <xs:complexContent>
      <xs:extension base="mbsim:BodyType">
        <xs:sequence>
          <xs:element name="mass" type="pv:massScalar">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                Masse des Körpers.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="positionIntegral" type="pv:unknownVector">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                Integral des Ortsvektors über die Masse, <object class="inlineeqn">\int \boldsymbol{r}\,\mathrm{d}m</object>.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="positionPositionIntegral" type="pv:inertiaMatrix">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                Integral des Produkts aus dem Ortsvektor mit der Transponierten des Ortsvektors über die Masse, <object class="inlineeqn">\int \boldsymbol{r}\,\boldsymbol{r}^T\,\mathrm{d}m</object>.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="shapeFunctionIntegral" type="pv:unknownMatrix" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                Integral der Matrix der Ansatzfunktionen über die Masse, <object class="inlineeqn">\int \boldsymbol{\Phi}\,\mathrm{d}m</object>.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="positionShapeFunctionIntegralArray" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                3x1-dimensionales Feld aus Integralen des Produkts aus den Elementen des Ortsvektors mit der Matrix der Ansatzfunktionen über die Masse, <object class="inlineeqn">\int r_i\,\boldsymbol{\Phi}\,\mathrm{d}m,\ i=1,2,3</object>.
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ele" type="pv:unknownMatrix" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="shapeFunctionShapeFunctionIntegralArray" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                3x3-dimensionales Feld aus Integralen des Produkts aus den Spalten der Matrix der Ansatzfunktionen mit den Transponierten der Spalten der Matrix der Ansatzfunktionen über die Masse, <object class="inlineeqn">\int \boldsymbol{\Phi}_i\,\boldsymbol{\Phi}^T_j\,\mathrm{d}m,\ i,j=1,2,3</object>.
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="row" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element name="ele" type="pv:unknownMatrix" maxOccurs="unbounded"/>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="stiffnessMatrix" type="pv:unknownMatrix" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                Steifigkeistmatrix.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="dampingMatrix" type="pv:unknownMatrix" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                Dämpfungsmatrix.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="modalDamping" type="pv:nounitVector" minOccurs="0">
            <xs:annotation><xs:documentation xml:lang="de" xmlns="">
              Definiert den Dämpfungsgrad für jede Mode.
            </xs:documentation></xs:annotation>
          </xs:element>
          <xs:element name="proportionalDamping" type="pv:nounitVector" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                Proportionale Dämpfung, <object class="inlineeqn">\boldsymbol{D} = \beta_1\,\boldsymbol{M} + \beta_2\,\boldsymbol{K}</object>.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="nonlinearStiffnessMatrixOfFirstOrderArray" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                nx1-dimensionales Feld aus Matrizen zur Berücksichtigung nichtlinearer Verzerrungen erster Ordnung.
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ele" type="pv:unknownMatrix" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="nonlinearStiffnessMatrixOfSecondOrderArray" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                nxn-dimensionales Feld aus Matrizen zur Berücksichtigung nichtlinearer Verzerrungen zweiter Ordnung.
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="row" maxOccurs="unbounded">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element name="ele" type="pv:unknownMatrix" maxOccurs="unbounded"/>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="initialStressIntegral" type="pv:unknownVector" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                Vektor der generalisierte Kräfte infolge von Vorspannungen.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="nonlinearInitialStressIntegral" type="pv:unknownMatrix" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                Matrix zur Berücksichtigung der generalisierte Kräfte infolge von Vorspannungen bei nichtlinearen Verzerrungen.
              </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="geometricStiffnessMatrixDueToAccelerationArray" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                3x1-dimensionales Feld aus Matrizen zu Berücksichtigung geometrischer Steifigkeiten infolge von Beschleunigungen.
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ele" type="pv:unknownMatrix" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="geometricStiffnessMatrixDueToAngularAccelerationArray" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                3x1-dimensionales Feld aus Matrizen zu Berücksichtigung geometrischer Steifigkeiten infolge von Winkelbeschleunigungen.
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ele" type="pv:unknownMatrix" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="geometricStiffnessMatrixDueToAngularVelocityArray" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                3x1-dimensionales Feld aus Matrizen zu Berücksichtigung geometrischer Steifigkeiten infolge von Winkelgeschwindigkeiten.
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ele" type="pv:unknownMatrix" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="nodeNumbers" type="pv:nounitVector" minOccurs="0"/>
          <xs:element name="nodalRelativePositionArray" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
                tbd
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ele" type="pv:lengthVector" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="nodalRelativeOrientationArray" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
              tbd
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ele" type="pv:rotationMatrix" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="nodalShapeMatrixOfTranslationArray" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
              tbd
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ele" type="pv:unknownMatrix" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="nodalShapeMatrixOfRotationArray" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
              tbd
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ele" type="pv:unknownMatrix" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="nodalStressMatrixArray" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
              tbd
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ele" type="pv:unknownMatrix" maxOccurs="unbounded"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="nodalNonlinearStressMatrixArray" minOccurs="0">
            <xs:annotation>
              <xs:documentation xml:lang="de" xmlns="">
              tbd
              </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="row" maxOccurs="unbounded">
