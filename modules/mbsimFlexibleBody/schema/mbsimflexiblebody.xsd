                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:group ref="CommonFlexibleFfrBody"/>
          <xs:sequence minOccurs="0">
            <xs:element name="openMBVFlexibleBody">
