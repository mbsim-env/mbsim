            </xs:annotation>
            <xs:complexType>
              <xs:simpleContent>
                <xs:extension base="pv:booleanFullEval">
                  <xs:attribute name="value" type="pv:qnamePartialEval" use="required"/>
                </xs:extension>
              </xs:simpleContent>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

