<Group environment="True" name="env" xmlns="http://www.mbsim-env.de/OpenMBV">
  <Rotation name="rotation">
    <initialTranslation>[0,-3,0]</initialTranslation>
    <initialRotation>[0,0,0]</initialRotation>
    <scaleFactor>1</scaleFactor>
    <startAngle>0.5</startAngle>
    <endAngle>1</endAngle>
    <contour>
      [ [0, 0, 1],
        [1, 0, 1],
        [1, 1, 1] ]
    </contour>
  </Rotation>
  <Extrusion name="extrusion">
    <initialTranslation>[2,-3,0]</initialTranslation>
    <initialRotation>[0,0,0]</initialRotation>
    <scaleFactor>1</scaleFactor>
    <windingRule>"odd"</windingRule>
    <height>0.5</height>
    <contour>
      [ [0, 0, 1],
        [1, 0, 1],
        [1, 1, 1] ]
    </contour>
    <contour>
      [ [0.6, 0.2, 1],
        [0.8, 0.2, 1],
        [0.8, 0.4, 1],
        [0.6, 0.4, 1] ]
    </contour>
  </Extrusion>
  <SpineExtrusion name="splineextrusionortho">
    <contour>
      [ [+0.1, +0.2, 1],
        [+0.1, -0.2, 1],
        [-0.1, -0.2, 1],
        [-0.1, +0.2, 1] ]
    </contour>
    <crossSectionOrientation>'orthogonalWithTwist'</crossSectionOrientation>
    <scaleFactor>1</scaleFactor>
    <initialRotation>[0,0,0]</initialRotation>
    <stateOffSet>
      import numpy
      ret=[]
      r=1.5
      for a in numpy.linspace(0,math.pi,20):
        ret.extend([-r*math.sin(a), r*math.cos(a), 0, a])
    </stateOffSet>
  </SpineExtrusion>
  <SpineExtrusion name="splineextrusionfree">
    <contour>
      import numpy
      ret = [
        [0.0,0.4,1],
        [0.0,0.0,1],
        [0.2,0.0,1],
      ]
      for a in numpy.linspace(0,math.pi/2,10)[1:-1]:
        ret.append([0.2*math.cos(a),0.4*math.sin(a),0])
      ret=numpy.flip(ret,axis=0)
    </contour>
    <crossSectionOrientation>'cardanWrtWorld'</crossSectionOrientation>
    <scaleFactor>2</scaleFactor>
    <initialRotation>[0,0,0]</initialRotation>
    <stateOffSet>
      import numpy
      ret=[]
      r=2.5
      for a in numpy.linspace(0,math.pi,20):
        ret.extend([-r*math.sin(a), r*math.cos(a), 0,
                    0,45*math.pi/180,math.pi/2+a])
    </stateOffSet>
  </SpineExtrusion>
</Group>
