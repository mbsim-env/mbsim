<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<MBSimProject xmlns="http://www.mbsim-env.de/MBSimXML" name="Project">

  <Embed xmlns="http://www.mbsim-env.de/MBXMLUtils">
    <Parameter>
      <scalarParameter name="l" unit="-">1</scalarParameter>
      <scalarParameter name="b" unit="-">0</scalarParameter>
      <scalarParameter name="al" unit="-">70/180*pi</scalarParameter>
      <vectorParameter name="q0" unit="-">
        <xmlVector>
          <ele>l/2*cos(al)</ele>
          <ele>l/2*sin(al)*1.0001</ele>
          <ele>-al</ele>
        </xmlVector>
      </vectorParameter>
      <scalarParameter name="mu" unit="-">0.2</scalarParameter>
      <scalarParameter name="a" unit="-">l*cos(al)</scalarParameter>
      <scalarParameter name="h" unit="-">l*sin(al)</scalarParameter>
      <scalarParameter name="xG" unit="-">1/(1+mu^2)*(a-mu*h)</scalarParameter>
    </Parameter>
    <DynamicSystemSolver xmlns="http://www.mbsim-env.de/MBSim" name="MBS">
      <plotFeatureRecursive value="generalizedRelativePosition">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedRelativeVelocity">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedForce">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedPosition">true</plotFeatureRecursive>
      <plotFeatureRecursive value="generalizedVelocity">true</plotFeatureRecursive>
      <frames>
        <FixedRelativeFrame name="P1">
          <enableOpenMBV>
            <size unit="dm">1</size>
          </enableOpenMBV>
          <relativeOrientation>
            <aboutZ xmlns="http://www.mbsim-env.de/MBXMLUtils" unit="degree">90</aboutZ>
          </relativeOrientation>
        </FixedRelativeFrame>
      </frames>
      <contours>
        <Line name="Line1">
          <frameOfReference ref="../Frame[I]"/>
          <enableOpenMBV>
            <length unit="m">3</length>
          </enableOpenMBV>
        </Line>
        <Line name="Line2">
          <frameOfReference ref="../Frame[P1]"/>
          <enableOpenMBV>
            <length unit="m">3</length>
          </enableOpenMBV>
        </Line>
      </contours>
      <groups/>
      <objects>
        <Embed xmlns="http://www.mbsim-env.de/MBXMLUtils">
          <Parameter/>
          <RigidBody xmlns="http://www.mbsim-env.de/MBSim" name="Stepladder">
            <generalizedInitialPosition>
              <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                <ele>q0(1)</ele>
                <ele>q0(2)</ele>
                <ele>q0(3)</ele>
              </xmlVector>
            </generalizedInitialPosition>
            <mass unit="kg">1e-9</mass>
            <inertiaTensor unit="kg*m^2">
              <xmlMatrix xmlns="http://www.mbsim-env.de/MBXMLUtils">
                <row>
                  <ele>0.01</ele>
                  <ele>0</ele>
                  <ele>0</ele>
                </row>
                <row>
                  <ele>0</ele>
                  <ele>0.01</ele>
                  <ele>0</ele>
                </row>
                <row>
                  <ele>0</ele>
                  <ele>0</ele>
                  <ele>0.01</ele>
                </row>
              </xmlMatrix>
            </inertiaTensor>
            <stateDependentTranslation>
              <TranslationAlongAxesXY/>
            </stateDependentTranslation>
            <stateDependentRotation>
              <RotationAboutZAxis/>
            </stateDependentRotation>
            <frames>
              <FixedRelativeFrame name="P1">
                <enableOpenMBV>
                  <size unit="dm">1</size>
                </enableOpenMBV>
                <relativePosition unit="m">
                  <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                    <ele>-l/2</ele>
                    <ele>-b/2</ele>
                    <ele>0</ele>
                  </xmlVector>
                </relativePosition>
              </FixedRelativeFrame>
              <FixedRelativeFrame name="P2">
                <enableOpenMBV>
                  <size unit="dm">1</size>
                </enableOpenMBV>
                <relativePosition unit="m">
                  <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                    <ele>l/2</ele>
                    <ele>-b/2</ele>
                    <ele>0</ele>
                  </xmlVector>
                </relativePosition>
              </FixedRelativeFrame>
            </frames>
            <contours>
              <Point name="Point1">
                <frameOfReference ref="../Frame[P1]"/>
                <enableOpenMBV>
                </enableOpenMBV>
              </Point>
              <Point name="Point2">
                <frameOfReference ref="../Frame[P2]"/>
                <enableOpenMBV>
                </enableOpenMBV>
              </Point>
            </contours>
            <openMBVRigidBody>
              <Cuboid xmlns="http://www.mbsim-env.de/OpenMBV" name="Body1">
                <transparency unit="-">0.3</transparency>
                <initialTranslation unit="m">
                  <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                    <ele>0</ele>
                    <ele>0</ele>
                    <ele>0</ele>
                  </xmlVector>
                </initialTranslation>
                <initialRotation unit="rad">
                  <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                    <ele>0</ele>
                    <ele>0</ele>
                    <ele>0</ele>
                  </xmlVector>
                </initialRotation>
                <scaleFactor unit="-">1</scaleFactor>
                <length unit="m">
                  <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                    <ele>l</ele>
                    <ele>b</ele>
                    <ele>b</ele>
                  </xmlVector>
                </length>
              </Cuboid>
            </openMBVRigidBody>
            <enableOpenMBVFrameC>
              <size unit="dm">1</size>
            </enableOpenMBVFrameC>
          </RigidBody>
        </Embed>
        <RigidBody name="Mass">
          <frameOfReference ref="../Object[Stepladder]/Frame[P2]"/>
          <mass unit="kg">1</mass>
          <inertiaTensor unit="kg*m^2">
            <xmlMatrix xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <row>
                <ele>0.01</ele>
                <ele>0</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0.01</ele>
                <ele>0</ele>
              </row>
              <row>
                <ele>0</ele>
                <ele>0</ele>
                <ele>0.01</ele>
              </row>
            </xmlMatrix>
          </inertiaTensor>
          <timeDependentTranslation>
            <CompositeFunction>
              <outerFunction>
                <TranslationAlongXAxis/>
              </outerFunction>
              <innerFunction>
                <LinearFunction>
                  <a1>-0.1</a1>
                </LinearFunction>
              </innerFunction>
            </CompositeFunction>
          </timeDependentTranslation>
          <frames/>
          <contours/>
          <openMBVRigidBody>
            <Sphere xmlns="http://www.mbsim-env.de/OpenMBV" name="Body1">
              <transparency unit="-">0.3</transparency>
              <initialTranslation unit="m">
                <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                  <ele>0</ele>
                  <ele>0</ele>
                  <ele>0</ele>
                </xmlVector>
              </initialTranslation>
              <initialRotation unit="rad">
                <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
                  <ele>0</ele>
                  <ele>0</ele>
                  <ele>0</ele>
                </xmlVector>
              </initialRotation>
              <scaleFactor unit="-">1</scaleFactor>
              <radius unit="dm">0.5</radius>
            </Sphere>
          </openMBVRigidBody>
          <enableOpenMBVFrameC>
            <size unit="dm">1</size>
          </enableOpenMBVFrameC>
        </RigidBody>
      </objects>
      <links>
        <Contact name="Contact1">
          <connect ref1="../Contour[Line1]" ref2="../Object[Stepladder]/Contour[Point1]"/>
          <normalForceLaw>
            <UnilateralConstraint/>
          </normalForceLaw>
          <normalImpactLaw>
            <UnilateralNewtonImpact>
              <restitutionCoefficient unit="-">0</restitutionCoefficient>
            </UnilateralNewtonImpact>
          </normalImpactLaw>
          <tangentialForceLaw>
            <PlanarCoulombFriction>
              <frictionCoefficient unit="-">mu</frictionCoefficient>
            </PlanarCoulombFriction>
          </tangentialForceLaw>
          <tangentialImpactLaw>
            <PlanarCoulombImpact>
              <frictionCoefficient unit="-">mu</frictionCoefficient>
            </PlanarCoulombImpact>
          </tangentialImpactLaw>
        </Contact>
        <Contact name="Contact2">
          <connect ref1="../Contour[Line2]" ref2="../Object[Stepladder]/Contour[Point2]"/>
          <normalForceLaw>
            <UnilateralConstraint/>
          </normalForceLaw>
          <normalImpactLaw>
            <UnilateralNewtonImpact>
              <restitutionCoefficient unit="-">0</restitutionCoefficient>
            </UnilateralNewtonImpact>
          </normalImpactLaw>
          <tangentialForceLaw>
            <PlanarCoulombFriction>
              <frictionCoefficient unit="-">mu</frictionCoefficient>
            </PlanarCoulombFriction>
          </tangentialForceLaw>
          <tangentialImpactLaw>
            <PlanarCoulombImpact>
              <frictionCoefficient unit="-">mu</frictionCoefficient>
            </PlanarCoulombImpact>
          </tangentialImpactLaw>
        </Contact>
        <Contact name="Contact3">
          <connect ref1="../Contour[Line2]" ref2="../Object[Stepladder]/Contour[Point1]"/>
          <normalForceLaw>
            <UnilateralConstraint/>
          </normalForceLaw>
          <normalImpactLaw>
            <UnilateralNewtonImpact>
              <restitutionCoefficient unit="-">0</restitutionCoefficient>
            </UnilateralNewtonImpact>
          </normalImpactLaw>
          <tangentialForceLaw>
            <PlanarCoulombFriction>
              <frictionCoefficient unit="-">mu</frictionCoefficient>
            </PlanarCoulombFriction>
          </tangentialForceLaw>
          <tangentialImpactLaw>
            <PlanarCoulombImpact>
              <frictionCoefficient unit="-">mu</frictionCoefficient>
            </PlanarCoulombImpact>
          </tangentialImpactLaw>
        </Contact>
      </links>
      <constraints/>
      <observers>
        <FrameObserver name="FrameObserver1">
          <frame ref="../Object[Stepladder]/Frame[P1]"/>
        </FrameObserver>
        <FrameObserver name="FrameObserver2">
          <frame ref="../Object[Stepladder]/Frame[P2]"/>
        </FrameObserver>
        <RigidBodyObserver name="RigidBodyObserver1">
          <rigidBody ref="../Object[Mass]"/>
          <enableOpenMBVWeight>
            <scaleLength unit="-">0.1</scaleLength>
          </enableOpenMBVWeight>
        </RigidBodyObserver>
        <ContactObserver name="ContactObserver1">
          <contact ref="../Link[Contact1]"/>
          <enableOpenMBVNormalForce>
            <scaleLength unit="-">0.1</scaleLength>
          </enableOpenMBVNormalForce>
          <enableOpenMBVTangentialForce>
            <scaleLength unit="-">0.1</scaleLength>
          </enableOpenMBVTangentialForce>
        </ContactObserver>
        <ContactObserver name="ContactObserver2">
          <contact ref="../Link[Contact2]"/>
          <enableOpenMBVNormalForce>
            <scaleLength unit="-">0.1</scaleLength>
          </enableOpenMBVNormalForce>
          <enableOpenMBVTangentialForce>
            <scaleLength unit="-">0.1</scaleLength>
          </enableOpenMBVTangentialForce>
        </ContactObserver>
        <ContactObserver name="ContactObserver3">
          <contact ref="../Link[Contact3]"/>
          <enableOpenMBVNormalForce>
            <scaleLength unit="-">0.1</scaleLength>
          </enableOpenMBVNormalForce>
          <enableOpenMBVTangentialForce>
            <scaleLength unit="-">0.1</scaleLength>
          </enableOpenMBVTangentialForce>
        </ContactObserver>
      </observers>
      <enableOpenMBVFrameI>
        <size unit="dm">1</size>
      </enableOpenMBVFrameI>
      <environments>
        <MBSimEnvironment>
          <accelerationOfGravity unit="m/s^2">
            <xmlVector xmlns="http://www.mbsim-env.de/MBXMLUtils">
              <ele>0</ele>
              <ele>-9.81</ele>
              <ele>0</ele>
            </xmlVector>
          </accelerationOfGravity>
        </MBSimEnvironment>
        </environments>
        <projectionTolerance unit="-">1e-15</projectionTolerance>
        <generalizedRelativePositionTolerance unit="-">1e-6</generalizedRelativePositionTolerance>
        <generalizedRelativeVelocityTolerance unit="-">1e-8</generalizedRelativeVelocityTolerance>
        <generalizedRelativeAccelerationTolerance unit="-">1e-10</generalizedRelativeAccelerationTolerance>
        <generalizedForceTolerance unit="-">1e-10</generalizedForceTolerance>
        <generalizedImpulseTolerance unit="-">1e-8</generalizedImpulseTolerance>
      <useConstraintSolverForPlot>1</useConstraintSolverForPlot>
    </DynamicSystemSolver>
  </Embed>

  <LSODARIntegrator xmlns="http://www.mbsim-env.de/MBSimIntegrator">
    <startTime unit="s">0</startTime>
    <endTime unit="s">7</endTime>
    <plotStepSize unit="s">1e-2</plotStepSize>
    <absoluteToleranceScalar>1e-6</absoluteToleranceScalar>
    <relativeToleranceScalar unit="-">1e-6</relativeToleranceScalar>
    <initialStepSize unit="s">1e-10</initialStepSize>
    <maximumStepSize unit="s">0</maximumStepSize>
    <minimumStepSize unit="s">0</minimumStepSize>
    <plotOnRoot>0</plotOnRoot>
  </LSODARIntegrator>

</MBSimProject>
