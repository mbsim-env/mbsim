<?xml version="1.0" encoding="UTF-8"?>
<Group name="HydraulicSystem" xmlns="http://mbsim.berlios.de/MBSim">
  <frameOfReference ref="../Frame[I]"/>
  <frames/>
  <contours/>
  <groups>
    <Controlvalve43 name="Controlvalve" xmlns="http://mbsim.berlios.de/MBSimHydraulics">
      <length>.02</length>
      <diameter>.003</diameter>
      <alpha>.7</alpha>
      <relativeAlphaPA>
        <Function1_SS_from_VS xmlns="http://mbsim.berlios.de/MBSim">
          <TabularFunction1_VS>
            <x>[0; .4; .45; 1]</x>
            <y>[1;  1;  0; 0]</y>
          </TabularFunction1_VS>
        </Function1_SS_from_VS>
      </relativeAlphaPA>
      <minimalRelativeAlpha>.05</minimalRelativeAlpha>
      <bilateralConstrained/>
      <offset>.05</offset>
      <relativePosition ref="../../Group[Control]/Signal[PT1ControlvalveOutputLimitted]"/>
      <inflowP ref="../HLine[lineP]"/>
      <outflowA ref="../HLine[lineA]"/>
      <outflowB ref="../HLine[lineB]"/>
      <outflowT ref="../HLine[lineT]"/>
    </Controlvalve43>  
  </groups>
  <objects>
