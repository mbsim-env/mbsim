  <links>
    <ConstrainedNode name="sourceP" xmlns="http://mbsim.berlios.de/MBSimHydraulics">
      <outflow ref="../HLine[lineP]"/>
      <function>
        <ConstantFunction1_SS xmlns="http://mbsim.berlios.de/MBSim">
          <value>40e5</value>
        </ConstantFunction1_SS>
      </function>
    </ConstrainedNode>
    <EnvironmentNode name="sourceT" xmlns="http://mbsim.berlios.de/MBSimHydraulics">
      <inflow ref="../HLine[lineT]"/>
    </EnvironmentNode>
  </links>
</Group>
