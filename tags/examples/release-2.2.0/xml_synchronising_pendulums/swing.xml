<Group name="Swing" xmlns="http://mbsim.berlios.de/MBSim">
  <frameOfReference ref="../Frame[I]"/> <!-- optional -->
  <position>[0 0 0]'</position>
  <orientation>eye(3)</orientation>
  <frames>
    <frame>
      <Frame name="fMountLeft">
        <!--
           -<enableOpenMBV>
           -  <size>fSize</size>
           -  <offset>1.</offset>
           -</enableOpenMBV>
           -->
      </Frame>
      <frameOfReference ref="Frame[I]"/>
      <position>[-swingL/2; suspensionL; 0]</position>
      <orientation>rotateAboutZ(-pi/2)</orientation>
    </frame>
    <frame>
      <Frame name="fMountRight">
        <!--
           -<enableOpenMBV>
           -  <size>fSize</size>
           -  <offset>1.</offset>
           -</enableOpenMBV>
           -->
      </Frame>
      <frameOfReference ref="Frame[I]"/> <!-- optional -->
      <position>[swingL/2; suspensionL; 0]</position>
      <orientation>rotateAboutZ(-pi/2)</orientation>
    </frame>
  </frames>
  <contours>
  </contours>
  <groups>
  </groups>
  <objects>
    <RigidBody name="Swing" xmlns="http://mbsim.berlios.de/MBSim">
      <initialGeneralizedPosition>suspensionL*[sin(phi0); 1-cos(phi0)]</initialGeneralizedPosition>
      <initialGeneralizedVelocity>suspensionL*omega0*[cos(phi0); sin(phi0)]</initialGeneralizedVelocity>
      <frameOfReference ref="../Frame[I]"/>
      <frameForKinematics ref="Frame[C]"/>
      <mass>swingMass</mass>
      <inertiaTensor>swingTheta</inertiaTensor>
      <translation>
        <LinearTranslation xmlns="http://mbsim.berlios.de/MBSim">
          <translationVectors>[1 0 0; 0 1 0]'</translationVectors>
        </LinearTranslation>
      </translation>
      <rotation>
      </rotation>
      <frames>
        <frame>
          <Frame name="fMountLeft">
            <!--
               -<enableOpenMBV>
               -  <size>fSize</size>
               -  <offset>1</offset>
               -</enableOpenMBV>
               -->
          </Frame>
          <frameOfReference ref="Frame[C]"/>
          <position>[-swingL/2 0 0]'</position>
          <orientation>eye(3)</orientation>
        </frame>
        <frame>
          <Frame name="fMountRight">
            <!--
               -<enableOpenMBV>
               -  <size>fSize</size>
               -  <offset>1</offset>
               -</enableOpenMBV>
               -->
          </Frame>
          <frameOfReference ref="Frame[C]"/>
          <position>[swingL/2 0 0]'</position>
          <orientation>eye(3)</orientation>
        </frame>
        <frame>
          <Frame name="fH">
            <!--
               -<enableOpenMBV>
               -  <size>fSize</size>
               -  <offset>1</offset>
               -</enableOpenMBV>
               -->
          </Frame>
          <frameOfReference ref="Frame[C]"/>
          <position>[0 swingH/2 0]'</position>
          <orientation>rotateAboutZ(pi/2)</orientation>
        </frame>
      </frames>
      <contours>
        <contour>
          <Line name="cTop"/>
          <frameOfReference ref="Frame[fH]"/>
          <position>[0 0 0]'</position>
          <orientation>eye(3)</orientation>
        </contour>
      </contours>
      <openMBVRigidBody>
        <Cuboid name="X" xmlns="http://openmbv.berlios.de/OpenMBV">
          <staticColor>.2</staticColor>
          <initialTranslation>[0 0 0]'</initialTranslation>
          <initialRotation>[pi/2 0 0]'</initialRotation>
          <scaleFactor>1.</scaleFactor>
          <length>[swingL swingB swingH]'</length>
        </Cuboid>
        <frameOfReference ref="Frame[C]"/>
      </openMBVRigidBody>
      <!--
         -<enableOpenMBVFrameC>
         -  <size>fSize</size>
         -  <offset>1</offset>
         -</enableOpenMBVFrameC>
         -->
    </RigidBody>
    <RigidBody name="SuspensionLeft" xmlns="http://mbsim.berlios.de/MBSim">
      <initialGeneralizedPosition>phi0</initialGeneralizedPosition>
      <initialGeneralizedVelocity>omega0</initialGeneralizedVelocity>
      <frameOfReference ref="../Frame[fMountLeft]"/>
      <frameForKinematics ref="Frame[fMountLeft]"/>
      <mass>suspensionMass</mass>
      <inertiaTensor>suspensionTheta</inertiaTensor>
      <translation/>
      <rotation>
        <RotationAboutFixedAxis xmlns="http://mbsim.berlios.de/MBSim">
          <axisOfRotation>[0 0 1]'</axisOfRotation>
        </RotationAboutFixedAxis>
      </rotation>
      <frames>
        <frame>
          <Frame name="fMountLeft">
            <!--
               -<enableOpenMBV>
               -  <size>fSize</size>
               -  <offset>1</offset>
               -</enableOpenMBV>
               -->
          </Frame>
          <frameOfReference ref="Frame[C]"/>
          <position>[-suspensionL/2 0 0]'</position>
          <orientation>eye(3)</orientation>
        </frame>
        <frame>
          <Frame name="fMountRight">
            <!--
               -<enableOpenMBV>
               -  <size>fSize</size>
               -  <offset>1</offset>
               -</enableOpenMBV>
               -->
          </Frame>
          <frameOfReference ref="Frame[C]"/>
          <position>[suspensionL/2 0 0]'</position>
          <orientation>eye(3)</orientation>
        </frame>
      </frames>
      <contours/>
      <openMBVRigidBody>
        <Cuboid name="X" xmlns="http://openmbv.berlios.de/OpenMBV">
          <staticColor>.4</staticColor>
          <initialTranslation>[0 0 0]'</initialTranslation>
          <initialRotation>[pi/2 0 0]'</initialRotation>
          <scaleFactor>1.</scaleFactor>
          <length>[suspensionL suspensionB suspensionH]'</length>
        </Cuboid>
        <frameOfReference ref="Frame[C]"/>
      </openMBVRigidBody>
      <!--
         -<enableOpenMBVFrameC>
         -  <size>fSize</size>
         -  <offset>1</offset>
         -</enableOpenMBVFrameC>
         -->
    </RigidBody>
    <RigidBody name="SuspensionRight" xmlns="http://mbsim.berlios.de/MBSim">
      <initialGeneralizedPosition>phi0</initialGeneralizedPosition>
      <initialGeneralizedVelocity>omega0</initialGeneralizedVelocity>
      <frameOfReference ref="../Frame[fMountRight]"/>
      <frameForKinematics ref="Frame[fMountLeft]"/>
      <mass>suspensionMass</mass>
      <inertiaTensor>suspensionTheta</inertiaTensor>
      <translation/>
      <rotation>
        <RotationAboutFixedAxis xmlns="http://mbsim.berlios.de/MBSim">
          <axisOfRotation>[0 0 1]'</axisOfRotation>
        </RotationAboutFixedAxis>
      </rotation>
      <frames>
        <frame>
          <Frame name="fMountLeft">
            <!--
               -<enableOpenMBV>
               -  <size>fSize</size>
               -  <offset>1</offset>
               -</enableOpenMBV>
               -->
          </Frame>
          <frameOfReference ref="Frame[C]"/>
          <position>[-suspensionL/2 0 0]'</position>
          <orientation>eye(3)</orientation>
        </frame>
        <frame>
          <Frame name="fMountRight">
            <!--
               -<enableOpenMBV>
               -  <size>fSize</size>
               -  <offset>1</offset>
               -</enableOpenMBV>
               -->
          </Frame>
          <frameOfReference ref="Frame[C]"/>
          <position>[suspensionL/2 0 0]'</position>
          <orientation>eye(3)</orientation>
        </frame>
      </frames>
      <contours/>
      <openMBVRigidBody>
        <Cuboid name="X" xmlns="http://openmbv.berlios.de/OpenMBV">
          <staticColor>.4</staticColor>
          <initialTranslation>[0 0 0]'</initialTranslation>
          <initialRotation>[pi/2 0 0]'</initialRotation>
          <scaleFactor>1.</scaleFactor>
          <length>[suspensionL suspensionB suspensionH]'</length>
        </Cuboid>
        <frameOfReference ref="Frame[C]"/>
      </openMBVRigidBody>
      <!--
         -<enableOpenMBVFrameC>
         -  <size>fSize</size>
         -  <offset>1</offset>
         -</enableOpenMBVFrameC>
         -->
    </RigidBody>
  </objects>
  <extraDynamics>
  </extraDynamics>
  <links>
    <Joint name="JointMountLeft" xmlns="http://mbsim.berlios.de/MBSim">
      <force>
        <direction>[1 0 0; 0 1 0]'</direction>
        <generalizedForceLaw>
          <BilateralConstraint xmlns="http://mbsim.berlios.de/MBSim"/>
        </generalizedForceLaw>
        <generalizedImpactLaw>
          <BilateralImpact xmlns="http://mbsim.berlios.de/MBSim"/>
        </generalizedImpactLaw>
      </force>
      <connect ref1="../Object[Swing]/Frame[fMountLeft]" ref2="../Object[SuspensionLeft]/Frame[fMountRight]"/>
    </Joint>
    <Joint name="JointMountRight" xmlns="http://mbsim.berlios.de/MBSim">
      <force>
        <direction>[1 0 0; 0 1 0]'</direction>
        <generalizedForceLaw>
          <BilateralConstraint xmlns="http://mbsim.berlios.de/MBSim"/>
        </generalizedForceLaw>
        <generalizedImpactLaw>
          <BilateralImpact xmlns="http://mbsim.berlios.de/MBSim"/>
        </generalizedImpactLaw>
      </force>
      <connect ref1="../Object[Swing]/Frame[fMountRight]" ref2="../Object[SuspensionRight]/Frame[fMountRight]"/>
    </Joint>
  </links>
</Group>

